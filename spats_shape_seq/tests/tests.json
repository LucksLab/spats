                { "id": "muts_cotrans_10", "comment": "corner case for find_partial, short target match", "r1": "TCCGGTCCTTGGTGCCCGAGTCAGATGTAGATCGGAAGAGCACACGTCTGAACTCCAGTCACATCACGATCTCGT", "r2": "ACATCTGACTCGGGCACCAAGGACCGGAAGATCGGAAGAGCGTCGTGTAGGGAAAGAGTGTAGATCTCGGTGGTC", "end": null, "site": null, "muts": [] },
